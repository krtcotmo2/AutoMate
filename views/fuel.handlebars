<p>{{title}}</p>
<script type="text/javascript" src="./assets/js/fuelLogic.js"></script>
<form>
     <div class="form-group" id = "fuelEntry">
          <label for="exampleInputEmail1">Date: </label>
          <input type="text" class="form-control" id="date" aria-describedby="calendar" placeholder="Date"
               value="{{mileage}}">
     </div>
     <div class="form-group">
          <label for="exampleInputPassword1">Odometer: </label>
          <input type="text" class="form-control" id="odometer" placeholder="Current Mileage" value="{{odometer}}">
     </div>
     <div class="form-group">
          <label for="exampleInputPassword1">Gallons: </label>
          <input type="text" class="form-control" id="gallons" placeholder="Gallons" value="{{licPlate}}">
     </div>
     <div class="form-group">
          <label for="exampleInputEmail1">$ per Gallon: </label>
          <input type="text" class="form-control" id="price" aria-describedby="dollar amount" placeholder="$ per Gallon"
               value="{{price}}">

     </div>
     <div class="form-group">
          <label for="exampleInputPassword1">Fuel Grade: </label>
          <input type="text" class="form-control" id="fuelGrade" placeholder="Fuel Grade" value="{{fuelGrade}}">
     </div>
     <div class="form-group">
          <label for="exampleInputPassword1">Gas Station</label>
          <input type="text" class="form-control" id="location" placeholder="Gas Station" value="{{gasStation}}">
     </div>
     <button type="submit" class="btn btn-primary" id="fuelSubmit">Submit</button>
</form>